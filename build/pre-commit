#!/bin/bash
funmd5_1() {
find /vendor -name "vendor.json" -type f | xargs md5sum > /tmp/funmd5_1.log
}
funmd5_2() {
find /vendor -name "vendor.json" -type f | xargs md5sum > /tmp/funmd5_2.log
}
cd ../..
if [ ! -f /tmp/funmd5_1.log ];then
funmd5_1
fi
funmd5_2
diff /tmp/funmd5_1.log /tmp/funmd5_2.log > /tmp/diff.log
Status=$?
if [ $Status = 0 ];then
exit
else
echo "vendor is change"
fi
